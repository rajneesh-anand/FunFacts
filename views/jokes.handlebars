<div class="section">
    <div class="row">
        {{#each memes}}
        <div class="col s12 m4">
            <div class="card">
                <div class="card-image">
                    <img src="{{this.image_url}}">
                    <span class="card-title">{{this.title}}</span>
                </div>
                <div class="card-content">
                    <p>{{this.description}}</p>
                </div>
                <div class="card-action">
                    <a href="/">This is a link</a>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>